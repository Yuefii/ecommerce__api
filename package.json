{
  "name": "starter-express-typescript",
  "version": "1.0.0",
  "author": "yuefii",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "dev": "nodemon",
    "start": "node dist/src/index.js",
    "build": "tsc",
    "postinstall": "prisma generate",
    "lint": "pnpx eslint .",
    "lint:fix": "pnpx eslint . --fix",
    "format": "pnpx prettier . --write",
    "prepare": "pnpm exec husky init",
    "check-types": "npx tsc --noEmit --pretty",
    "check-lint": "pnpx eslint .",
    "check-format": "pnpx prettier . --check"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "dependencies": {
    "@prisma/client": "5.14.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-fileupload": "^1.5.0",
    "firebase": "^10.12.1",
    "firebase-admin": "^12.1.1",
    "jsonwebtoken": "^9.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@eslint/js": "^9.2.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.26",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "eslint": "^9.2.0",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "prisma": "^5.14.0",
    "supertest": "^7.0.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.8.0"
  }
}